
.tabbar{
   position: fixed;
   bottom: 0;
   left: 0;
   width: 100%;
   height: 120rpx;
   display: flex;
   justify-content: space-around;
   padding-bottom: env(safe-area-inset-bottom);
   background: #fff;
   box-shadow:0px 0px 12rpx 0px rgba(0,0,0,0.08);
}
.image{
   width: 60rpx;
   height: 60rpx;
   vertical-align: middle;
}
.text{
   font-size: 24rpx;
   line-height: 32rpx;
   text-align: center;
   margin-top: 10rpx;
}
.tabitem{
   display: flex;
   flex-direction: column;
   justify-content: center;
}
.imgbox{
   align-self: center;
}
.active{
   color: #4EBA9A;
}